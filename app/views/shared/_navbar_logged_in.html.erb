<div class="navbar-wagon">
  <!-- Logo -->
  <%= link_to image_tag("logo.svg", class: "navbar-wagon-brand"), root_path %>



<div class="navbar-wagon-right hidden-xs hidden-sm">
  <div class="svg-wrapper_logged_in">
      <%= link_to user_path(current_user), class: "navbar-wagon-item navbar-wagon-link" do %>
        <svg height="40" width="90" xmlns="http://www.w3.org/2000/svg">
          <rect id="shape_logged_in" height="40" width="90"/>
          <div id="text">
            <span class="spot"></span>Profile
          </div>
        </svg>
      <% end %>
    </div>

  <div class="svg-wrapper_logged_in">
      <%= link_to jams_path, class: "navbar-wagon-item navbar-wagon-link" do %>
        <svg height="40" width="90" xmlns="http://www.w3.org/2000/svg">
          <rect id="shape_logged_in" height="40" width="90"/>
          <div id="text">
            <span class="spot"></span>Jams
          </div>
        </svg>
      <% end %>
    </div>

    <div class="svg-wrapper_logged_in">
      <%= link_to users_path, class: "navbar-wagon-item navbar-wagon-link" do %>
        <svg height="40" width="90" xmlns="http://www.w3.org/2000/svg">
          <rect id="shape_logged_in" height="40" width="90"/>
          <div id="text">
            <span class="spot"></span>Search
          </div>
        </svg>
      <% end %>
    </div>
    <div class="svg-wrapper_logged_in">
      <%= link_to destroy_user_session_path, method: :delete, class: "navbar-wagon-item navbar-wagon-link" do %>
        <svg height="40" width="90" xmlns="http://www.w3.org/2000/svg">
          <rect id="shape_logged_in" height="40" width="90" />
          <div id="text">
            <span class="spot"></span>Sign out
          </div>
        </svg>
      <% end %>
    </div>
      <%= link_to user_path(current_user) do %>
      <%= cl_image_tag current_user.photo, class: "profilepic" %>
    <% end %>

  </div>
</div>
